Cell[BoxData[{
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}], "\n", 
 RowBox[{
  RowBox[{"derivmatrx", "[", "n_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"a", ",", "b"}], "}"}], ",", 
    RowBox[{
     RowBox[{"a", "=", 
      RowBox[{"DiagonalMatrix", "[", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", "n", "}"}]}], "]"}], "]"}]}], ";", 
     RowBox[{"b", "=", 
      RowBox[{
       RowBox[{"DiagonalMatrix", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{"1", ",", 
           RowBox[{"{", 
            RowBox[{"n", "-", "3"}], "}"}]}], "]"}], ",", 
         RowBox[{"-", "1"}]}], "]"}], "+", 
       RowBox[{"DiagonalMatrix", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{"1", ",", 
           RowBox[{"{", 
            RowBox[{"n", "-", "3"}], "}"}]}], "]"}], ",", "1"}], "]"}], "+", 
       RowBox[{"DiagonalMatrix", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"-", "2"}], ",", 
          RowBox[{"{", 
           RowBox[{"n", "-", "2"}], "}"}]}], "]"}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"j", "=", "2"}], ",", 
       RowBox[{"j", "<", "n"}], ",", 
       RowBox[{"j", "++"}], ",", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "2"}], ",", 
         RowBox[{"i", "<", "n"}], ",", 
         RowBox[{"i", "++"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{"a", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "j"}], "]"}], "]"}], "=", 
           RowBox[{"b", "[", 
            RowBox[{"[", 
             RowBox[{
              RowBox[{"i", "-", "1"}], ",", 
              RowBox[{"j", "-", "1"}]}], "]"}], "]"}]}], ";"}]}], "]"}]}], 
      "]"}], ";", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}], "=", "1"}], ";", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", 
        RowBox[{"2", ",", "1"}], "]"}], "]"}], "=", "1"}], ";", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"n", "-", "1"}], ",", "n"}], "]"}], "]"}], "=", "1"}], ";", 
     RowBox[{
      RowBox[{"a", "[", 
       RowBox[{"[", 
        RowBox[{"n", ",", "n"}], "]"}], "]"}], "=", "1"}], ";", "a"}]}], 
   "]"}]}]}], "Code",
